#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.2_pg@8.11.3_sqlite3@5.1.7_ts-node@10.9.2/node_modules/typeorm/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.2_pg@8.11.3_sqlite3@5.1.7_ts-node@10.9.2/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.2_pg@8.11.3_sqlite3@5.1.7_ts-node@10.9.2/node_modules/typeorm/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.2_pg@8.11.3_sqlite3@5.1.7_ts-node@10.9.2/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
