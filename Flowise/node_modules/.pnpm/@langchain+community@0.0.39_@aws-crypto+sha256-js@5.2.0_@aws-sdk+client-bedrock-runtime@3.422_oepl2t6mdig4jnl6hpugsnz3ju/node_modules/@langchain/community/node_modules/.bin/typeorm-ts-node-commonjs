#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules/typeorm/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules/typeorm/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_mysql2@3.9.2_pg@8.11.3_redis@4.6.13/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
