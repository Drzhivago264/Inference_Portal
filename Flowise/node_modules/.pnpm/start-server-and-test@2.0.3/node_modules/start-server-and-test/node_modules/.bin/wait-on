#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/bin/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/bin/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/wait-on@7.2.0_debug@4.3.4/node_modules:/home/h/Desktop/Inference_Portal/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/bin/wait-on" "$@"
else
  exec node  "$basedir/../../../../../wait-on@7.2.0_debug@4.3.4/node_modules/wait-on/bin/wait-on" "$@"
fi
